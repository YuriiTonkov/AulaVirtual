<Alloy>
	<Window id="winNuevaNota" class="container">
		<TableView top="10%" id="Marco">
			<TableViewRow height="40dp">
       			 <Label class="labelAlumno">Fecha:</Label>
        		 <Label class="textboxAlumno"  text="Pulse aqui" id="dateTextField"/>
        	</TableViewRow>
        	<TableViewRow height="40dp">
		        <Label class="labelAlumno">Titulo:</Label>
		        <TextField class="textboxAlumno" enabled="false"  id="txtTitulo"/>
	        </TableViewRow>
	        	<TableViewRow height="300dp">
			        <Label class="labelAlumno" >Observaciones:</Label>
			        <TextField class="textboxAlumno" height="100%" width="80%" enabled="false"  id="txtObservaciones"/>
	        </TableViewRow>
       	</TableView>
        <Button class="button" top="-50dp" id="btnGuardar" title="Guardar" onClick="GuardarExamen"></Button>
        <Button class="button" top="10dp" id="btnEnviar" title="Enviar" onClick="EnviarExamen"></Button>
        <Button class="button" top="-90dp" id="cancel" title="Cancelar" style="Titanium.UI.iPhone.SystemButtonStyle.BORDERED"></Button>
        <Button class="button" top="-100dp" id="done" title="Hecho" style="Titanium.UI.iPhone.SystemButtonStyle.BORDERED"></Button>
    </Window>
</Alloy>