<Alloy>
	<Window id="winNuevaAsignatura" class="container">
		<Label class="label" top="5%">Seleccione la asignatura</Label>
		<TableView dataCollection="Asignatura" id="TablaAsignaturas" allowsSelection="true">
			<TableViewRow  class="fila" data="{IdAsignatura}" id="rowAsignaturas" title="{Nombre}"></TableViewRow>
		</TableView>
		<Button class="button" id="btnGuardar" top="-50dp"title="Guardar" onClick="GuardarAsignatura"></Button>
	</Window>
</Alloy>