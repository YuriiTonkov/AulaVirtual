<Alloy>
    <Window id="winNuevaEvaluacion" class="container">
    	<TableView id="Formulario">
    		<TableViewRow height="40dp">
	        	<Label class="labelAlumno" >Nombre</Label>
	        	<TextField class="textboxAlumno" editable="false" id="txtNombreEvaluacion"></TextField>
	        </TableViewRow>
	        <TableViewRow height="40dp">
		        <Label class="labelAlumno" >Fecha Inicio</Label>
		        <TextField class="textboxAlumno"  editable="false" id="lblFecha"></TextField>
	         </TableViewRow>
		     <TableViewRow height="40dp">
		        <Label class="labelAlumno" >Peso</Label>
	       		 <TextField class="textboxAlumno" editable="false"  id="txtPeso"></TextField>
	         </TableViewRow>
	        <TableViewRow height="40dp">
		        <Label class="labelAlumno" >Nota</Label>
		        <Label class="textboxAlumno"  id="txtNota"></Label>
		    </TableViewRow>
		 </TableView>
	     <Button class="button" top="-50dp" id="btnGuardar" title="Guardar"></Button>
	     <Button class="button" top="-90dp" id="cancel" title="Cancelar" style="Titanium.UI.iPhone.SystemButtonStyle.BORDERED"></Button>
	     <Button class="button" top="-100dp" id="done" title="Hecho" style="Titanium.UI.iPhone.SystemButtonStyle.BORDERED"></Button>
    	
    </Window>
</Alloy>